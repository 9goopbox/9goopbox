<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="joinemp">

	<insert id="join" parameterType="empvo">
  		insert into employee values(employee_seq.nextVal,#{id},#{pwd},#{name},#{tel},#{post},#{addr1},#{addr2},sysdate)
  	</insert>
  	
  	<select id="idChk" resultType="int" parameterType="String">
  		select count(*) cnt from employee where id=#{id}
  	</select>
  	
</mapper>