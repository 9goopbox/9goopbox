<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="receive">
	
	<insert id="patiok" parameterType="recvo" useGeneratedKeys="true" keyProperty="id">
		insert into patient values(Patient_sequence.nextVal, #{name}, #{age}, #{sex}, null, null, null, null, null)    
	</insert>
	
	<insert id="recok" parameterType="recvo">
		insert into treatment values(treatment_sequence.nextVal ,#{patinet_id}, 0, null, '초진', #{med_trtmt})
	</insert>
	
</mapper>