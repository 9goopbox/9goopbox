<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean name="/inventory_add.box" class="org.springframework.web.servlet.mvc.ParameterizableViewController">
		<property name="viewName" value="inventory/inventory_add"/>
	</bean>

	<bean name="/inventory_list.box" class="org.springframework.web.servlet.mvc.ParameterizableViewController">
		<property name="viewName" value="inventory/inventory_list"/>
	</bean>
	
</beans>
