<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean name="/inquiry_outpatient.box" class="org.springframework.web.servlet.mvc.ParameterizableViewController">
		<property name="viewName" value="inquiry/outpatient"/>
	</bean>
	
	<bean name="/inquiry_ward.box" class="org.springframework.web.servlet.mvc.ParameterizableViewController">
		<property name="viewName" value="inquiry/inquiry_ward"/>
	</bean>
</beans>
